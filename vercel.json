{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
      "crons": [
        {
          "path": "/tweet",
          "schedule": "0 9,12,3 * * *"
        }
      ]
  }
